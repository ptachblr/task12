per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
max_val = max(per_cent.values())
a = dict.values(per_cent)
money = input ("Введите сумму депозита: ")
c = float(money)
deposit = [a]
for i in a:
    deposit.append(i * c / 100)
deposit.pop(0)
print("Сумма, которую вы получите в каждом из банков —", deposit)
max_deposit = max_val * c / 100
float_max_deposit = float(max_deposit)
print("Максимальная сумма, которую вы можете заработать —", (float_max_deposit))
